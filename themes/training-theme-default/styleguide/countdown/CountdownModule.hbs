<ps-countdown-module class="CountdownModule">
   {{#with image}}
   {{#set imageSize='countdownBanner'}}
   <div class="CountdownModule-image">{{this}}</div>
   {{/set}}
   {{/with}}
   {{#with endDate}}
   <div class="CountdownModule-countdown" data-end-date="{{this}}"><span
           class="CountdownModule-countdown-days"></span>&nbsp;{{format "countdownText"}}</div>
   {{/with}}
   {{#with description}}
   <div class="CountdownModule-description">
       {{this}}
   </div>
   {{/with}}
   {{#with villains}}
   <div class="CountdownModule-villains">
       {{#each this}}
       <div class="CountdownModule-villains-villain">
           {{this}}
       </div>
       {{/each}}
   </div>
   {{/with}}
</ps-countdown-module>